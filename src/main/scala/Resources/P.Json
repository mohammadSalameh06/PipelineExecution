{
  "stages": [
    {
      "name": "stage",
      "isParallel": true,
      "jobs": [
        {
          "name": "job1",
          "jar": "JarFile.jar",
          "args": [
            "-jar"
          ],


          "logPath"  : "LOG/path"


        }
      ]
    },
    {
      "name": "stage 2",
      "isParallel": false,
      "jobs": [
        {
          "name": "job2",
          "jar": "JarFile.jar",
          "args": [
            "-i", "./input",
            "-o", "./output"
          ],


          "logPath"  : "LOG/path"

        },
        {
          "name": "job3+",
          "jar": "JarFile.jar",
          "args": [
            "-i", "./input",
            "-o", "./output"
          ],


          "logPath"  : "LOG/path"

        }
      ]
    }
  ]
}